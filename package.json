{
  "name": "node-matrix-assets-xml",
  "version": "1.0.0",
  "description": "Generate XML for Squiz Matrix' \"Import Assets from XML Tool\" from asset trees",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib",
    "pretest": "standard",
    "test": "tape -r babel-register test.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshgillies/node-matrix-assets-xml.git"
  },
  "author": "Josh Gillies <npm@joshgilli.es> (http://joshgilli.es/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshgillies/node-matrix-assets-xml/issues"
  },
  "homepage": "https://github.com/joshgillies/node-matrix-assets-xml#readme",
  "dependencies": {
    "node-matrix-importer": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.21",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "node-matrix-assets": "^3.0.0",
    "standard": "^5.4.1",
    "tape": "^4.4.0",
    "xml2js": "^0.4.15"
  }
}
